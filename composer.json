{
    "name": "tianyong90/lucksdk",
    "description": "纳客会员管理软件对接开发SDK",
    "keywords": [
        "lucksdk",
        "lucksoft",
        "纳客会员"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "tianyong90",
            "email": "412039588@qq.com"
        }
    ],
    "require": {
        "php":">=5.4",
        "ext-mcrypt": "*",
        "pimple/pimple": "~3.0",
        "guzzlehttp/guzzle": "^6.1",
        "monolog/monolog": "^1.17",
        "doctrine/cache": "~1.4",
        "symfony/http-foundation": "~2.6|~2.7|~2.8|~3.0",
        "symfony/var-dumper": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Tianyong90\\LuckSDK\\": "src/LuckSDK/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tianyong90\\LuckSDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
